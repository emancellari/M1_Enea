#include <iostream>
using namespace std;


// to init a 2 dimn array and display their values

const int row_size=3;
const int col_size=2;
int main() {

// 1. To declare an array and init the values

int test[row_size][col_size];

//.. To read the values from user

for(int i=0;i<row_size;i++){
  for(int j=0; j<col_size;j++){
  cin>> test[i][j];
  }
}


//2. Diplay the elements

for(int i=0;i<row_size;i++){
  for(int j=0; j<col_size;j++){
  cout<< test[i][j];
  cout<<"  ";
  }
  cout<<endl;
}

}